{
    "name": "meniam/app",
    "type": "library",
    "description": "Application library for PHP projects",
    "keywords": ["application","mvc"],
    "homepage": "http://github.com/meniam/app",
    "license": "MIT",
    "authors": [
        {
        "name": "Eugene Myazin",
        "email": "meniam@gmail.com",
        "homepage": "http://meniam.ru",
        "role": "Maintainer"
        }
    ],
    "require":{
        "php"           :">=5.4.0,<5.6",
        "ext-mbstring"  : "*",
        "ext-pdo_mysql" : "*",
        "lib-libxml"    : "*",
        "lib-iconv"     : "~1.14",
        "lib-pcre"      : "*",
        "zendframework/zendframework": "2.*"
    },

    "extra": {
        "branch-alias": {
            "dev-master": "2.2-dev",
            "dev-develop": "2.3-dev"
        }
    },

    "repositories":[
        {
            "type": "composer",
            "url": "https://packages.zendframework.com/"
        }
    ],
    "autoload": {
        "psr-0": {
            "App": "/library/"
        }
    }
}